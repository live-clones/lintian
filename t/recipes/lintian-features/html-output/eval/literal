    color: #333;
    font-size: 0.9em;
    text-decoration: none;
}

#nav a:hover {
    background-color: #F3F3F3;
    border-bottom: 2px solid #D70751;
}


/*
 * 4. Main content
 */

main {
    margin: 0 40px;
    font-size: 0.9em;
}

.showcase {
    margin: 25px 0;
    padding: 40px 20px;
