    border: 1px solid #DFE4E8;
}

.showcase p {
    margin-left: 160px;
    font-size: 1.1em;
}

/* For smaller devices, we leave out the "icon" - it is fairly wide with
 * little added value.
 */
@media (max-width: 700px) {

  .showcase {
      margin: 25px 0;
      padding: 20px;
      background: #EFF4F8;
      border: 1px solid #DFE4E8;
  }

