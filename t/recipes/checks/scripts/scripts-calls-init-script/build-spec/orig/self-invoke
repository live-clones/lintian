. /lib/lsb/init-functions

case "$1" in
  start)
    :
    ;;
  stop)
    :
    ;;
  restart)
    /etc/init.d/self-invoke start
    ;;
  status)
    :
    ;;
  force-reload)
    :
    ;;
esac
